@model IEnumerable<LostandFoundAnimals.Models.Post>
@{
    ViewData["Title"] = "Lost";
}
<h3>@ViewData["Message"]</h3>

<h2>Welcome</h2>
<div class="container">
    <div class="row">
        @foreach (var item in Model)
            {
        <div class="col-md-4">
            @Html.DisplayFor(modelItem => item.PostText)
            @Html.DisplayFor(modelItem => item.Address.Line1)

        </div>
            }
    </div>
</div>
<p>
    <a asp-action="Create">Create New</a>
</p>